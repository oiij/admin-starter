{
  "name": "@eiog/use",
  "type": "module",
  "version": "0.0.1",
  "description": "",
  "author": "Eiog",
  "license": "MIT",
  "homepage": "https://github.com/Eiog/vue3-starter-simple",
  "keywords": [
    "@eiog/use"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
