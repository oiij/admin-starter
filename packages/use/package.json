  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsc --noEmit && tsup"
  },
  "peerDependencies": {
    "@better-scroll/core": "^2.5.1",
    "@vueuse/core": "^12.0.0",
    "colord": "^2.9.3",
    "mixte": "3.0.0-beta.3",
    "nanoid": "^5.0.9",
    "nprogress": "^0.2.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
